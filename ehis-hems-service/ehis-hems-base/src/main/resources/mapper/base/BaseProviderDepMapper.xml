    <sql id="selectBaseProviderDepVo">
        select id, provider_code, visiting_type, first_dept, second_dept, visiting_floor, charactdept_flag, straightknot_flag, cartevital_flag, spprocurement_flag, callstarttime, callendtime, create_by, create_time, update_by, update_time, status from base_provider_dep
    </sql>

    <select id="selectBaseProviderDepList" parameterType="BaseProviderDep" resultMap="BaseProviderDepResult">
        <include refid="selectBaseProviderDepVo"/>
        <where>
            <if test="providerCode != null  and providerCode != ''"> and provider_code = #{providerCode}</if>
            <if test="visitingType != null  and visitingType != ''"> and visiting_type = #{visitingType}</if>
            <if test="firstDept != null  and firstDept != ''"> and first_dept = #{firstDept}</if>
            <if test="secondDept != null  and secondDept != ''"> and second_dept = #{secondDept}</if>
            <if test="visitingFloor != null  and visitingFloor != ''"> and visiting_floor = #{visitingFloor}</if>
            <if test="charactdeptFlag != null  and charactdeptFlag != ''"> and charactdept_flag = #{charactdeptFlag}</if>
            <if test="straightknotFlag != null  and straightknotFlag != ''"> and straightknot_flag = #{straightknotFlag}</if>
            <if test="cartevitalFlag != null  and cartevitalFlag != ''"> and cartevital_flag = #{cartevitalFlag}</if>
            <if test="spprocurementFlag != null  and spprocurementFlag != ''"> and spprocurement_flag = #{spprocurementFlag}</if>
            <if test="callstarttime != null "> and callstarttime = #{callstarttime}</if>
            <if test="callendtime != null "> and callendtime = #{callendtime}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>

    <select id="selectBaseProviderDepById" parameterType="String" resultMap="BaseProviderDepResult">
        <include refid="selectBaseProviderDepVo"/>
        where id = #{id}
    </select>

    <insert id="insertBaseProviderDep" parameterType="BaseProviderDep">
        insert into base_provider_dep
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="providerCode != null and providerCode != ''">provider_code,</if>
            <if test="visitingType != null">visiting_type,</if>
            <if test="firstDept != null and firstDept != ''">first_dept,</if>
            <if test="secondDept != null">second_dept,</if>
