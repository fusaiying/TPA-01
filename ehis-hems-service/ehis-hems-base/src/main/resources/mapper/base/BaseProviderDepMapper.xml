            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="status != null">#{status},</if>
        </trim>
    </insert>

    <update id="updateBaseProviderDep" parameterType="BaseProviderDep">
        update base_provider_dep
        <trim prefix="SET" suffixOverrides=",">
            <if test="providerCode != null and providerCode != ''">provider_code = #{providerCode},</if>
            <if test="visitingType != null">visiting_type = #{visitingType},</if>
            <if test="firstDept != null and firstDept != ''">first_dept = #{firstDept},</if>
            <if test="secondDept != null">second_dept = #{secondDept},</if>
