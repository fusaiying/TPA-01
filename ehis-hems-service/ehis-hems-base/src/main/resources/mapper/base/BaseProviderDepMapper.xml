    <sql id="selectBaseProviderDepVo">
        select serial_no, update_flag,provider_code, visiting_type,
        (SELECT DISTINCT
				dict_label from sys_dict_data
			WHERE
				dict_type = 'visiting_type' and dict_value = base_provider_dep_bak.visiting_type)  visitingTypeName,
        first_dept, second_dept, visiting_floor, charactdept_flag, straightknot_flag, cartevital_flag, spprocurement_flag, callstarttime, callendtime, create_by, create_time, update_by, update_time, status from base_provider_dep_bak
