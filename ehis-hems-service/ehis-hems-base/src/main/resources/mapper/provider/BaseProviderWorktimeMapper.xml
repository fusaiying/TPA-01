            <if test="providerCode != null and providerCode != ''">#{providerCode},</if>
            <if test="timeNode != null">#{timeNode},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="nodeRemark != null">#{nodeRemark},</if>
