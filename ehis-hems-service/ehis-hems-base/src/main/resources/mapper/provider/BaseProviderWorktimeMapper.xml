    <sql id="selectBaseProviderWorktimeVo">
        select id, provider_code, time_node, start_time, end_time, node_remark, status, create_by, create_time, update_by, update_time from base_provider_worktime
    </sql>

    <select id="selectBaseProviderWorktimeList" parameterType="BaseProviderWorktime" resultMap="BaseProviderWorktimeResult">
        <include refid="selectBaseProviderWorktimeVo"/>
