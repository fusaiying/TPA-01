    </sql>

    <select id="selectClaimRuleRelationInfoList" parameterType="com.paic.ehis.claimcal.domain.ClaimRuleRelation" resultMap="ClaimRuleRelationInfoResult">
        select relation.relation_no, relation.rule_no, relation.plan_code, plan.plan_Name,
        relation.duty_code, duty.duty_Name, relation.duty_detail_code, detail.duty_detail_Name,
        relation.feeitem_code, feeitem.feeitem_Name,
        relation.status, relation.create_by, relation.create_time, relation.update_by, relation.update_time
        from claim_rule_relation relation
        left join claim_product_plan plan on relation.plan_code = plan.plan_code
        left join claim_product_duty duty on relation.duty_code = duty.duty_code and relation.plan_code = duty.plan_code
        left join claim_product_duty_detail detail on relation.duty_detail_code = detail.duty_detail_code and relation.duty_code = detail.duty_code
        left join claim_product_feeitem feeitem on relation.feeitem_code = feeitem.feeitem_code and relation.duty_detail_code = feeitem.duty_detail_code
        <where>
            <if test="ruleNo != null  and ruleNo != ''"> and relation.rule_no = #{ruleNo}</if>
            <if test="planCode != null  and planCode != ''"> and relation.plan_code = #{planCode}</if>
            <if test="dutyCode != null  and dutyCode != ''"> and relation.duty_code = #{dutyCode}</if>
            <if test="dutyDetailCode != null  and dutyDetailCode != ''"> and relation.duty_detail_code = #{dutyDetailCode}</if>
            <if test="feeitemCode != null  and feeitemCode != ''"> and relation.feeitem_code = #{feeitemCode}</if>
            <if test="status != null  and status != ''"> and relation.status = #{status}</if>
        </where>
    </select>


