<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paic.ehis.cs.mapper.InterfaceMessageMapper">
    <resultMap type="com.paic.ehis.cs.domain.InterfaceMessage" id="InterfaceMessageResult">
        <result property="seqId"    column="seq_id"    />
        <result property="interfaceCode"    column="interface_code"    />
        <result property="applicationRole"    column="application_role"    />
        <result property="parameters"    column="parameters"    />
        <result property="body"    column="body"    />
        <result property="result"    column="result"    />
        <result property="successFlag"    column="success_flag"    />
        <result property="handleFlag"    column="handle_flag"    />
        <result property="status"    column="status"    />
        <result property="createdBy"    column="created_by"    />
        <result property="createdTime"    column="created_time"    />
        <result property="updatedBy"    column="updated_by"    />
        <result property="updatedTime"    column="updated_time"    />
    </resultMap>

    <sql id="selectInterfaceMessageVo">
        select seq_id, interface_code, application_role, parameters, body, result, success_flag, handle_flag, status, created_by, created_time, updated_by, updated_time
        from interfase_message
    </sql>

    <select id="selectInterfaceMessageList" parameterType="com.paic.ehis.cs.domain.InterfaceMessage" resultMap="InterfaceMessageResult">
        <include refid="selectInterfaceMessageVo"/>
        <where>
            <if test="seqId != null  and seqId != ''"> and seq_id = #{seqId}</if>
            <if test="interfaceCode != null  and interfaceCode != ''"> and interface_code = #{interfaceCode}</if>
            <if test="applicationRole != null  and applicationRole != ''"> and application_role = #{applicationRole}</if>
            <if test="parameters != null  and parameters != ''"> and parameters = #{parameters}</if>
            <if test="body != null  and body != ''"> and body = #{body}</if>
            <if test="result != null  and result != ''"> and result = #{result}</if>
            <if test="successFlag != null  and successFlag != ''"> and success_flag = #{successFlag}</if>
            <if test="handleFlag != null  and handleFlag != ''"> and handle_flag = #{handleFlag}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="createdBy != null  and createdBy != ''"> and created_by = #{createdBy}</if>
            <if test="createdTime != null "> and created_time = #{createdTime}</if>
            <if test="updatedBy != null  and updatedBy != ''"> and updated_by = #{updatedBy}</if>
            <if test="updatedTime != null "> and updated_time = #{updatedTime}</if>
        </where>
    </select>

    <select id="selectInterfaceMessageById" parameterType="String" resultMap="InterfaceMessageResult">
        <include refid="selectInterfaceMessageVo"/>
        where seq_id = #{seqId}
    </select>

    <insert id="insertInterfaceMessage" parameterType="com.paic.ehis.cs.domain.InterfaceMessage">
        insert into interfase_message
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="interfaceCode != null">interface_code,</if>
            <if test="applicationRole != null">application_role,</if>
            <if test="parameters != null">parameters,</if>
            <if test="body != null">body,</if>
            <if test="result != null">result,</if>
            <if test="successFlag != null">success_flag,</if>
            <if test="handleFlag != null">handle_flag,</if>
            <if test="status != null">status,</if>
            <if test="createdBy != null and createdBy != ''">created_by,</if>
            <if test="createdTime != null">created_time,</if>
            <if test="updatedBy != null and updatedBy != ''">updated_by,</if>
            <if test="updatedTime != null">updated_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="interfaceCode != null">#{interfaceCode},</if>
            <if test="applicationRole != null">#{applicationRole},</if>
            <if test="parameters != null">#{parameters},</if>
            <if test="body != null">#{body},</if>
            <if test="result != null">#{result},</if>
            <if test="successFlag != null">#{successFlag},</if>
            <if test="handleFlag != null">#{handleFlag},</if>
            <if test="status != null">#{status},</if>
            <if test="createdBy != null and createdBy != ''">#{createdBy},</if>
            <if test="createdTime != null">#{createdTime},</if>
            <if test="updatedBy != null and updatedBy != ''">#{updatedBy},</if>
            <if test="updatedTime != null">#{updatedTime},</if>
         </trim>
    </insert>

    <update id="updateInterfaceMessage" parameterType="com.paic.ehis.cs.domain.InterfaceMessage">
        update interfase_message
        <trim prefix="SET" suffixOverrides=",">
            <if test="interfaceCode != null">interface_code = #{interfaceCode},</if>
            <if test="applicationRole != null">application_role = #{applicationRole},</if>
            <if test="parameters != null">parameters = #{parameters},</if>
            <if test="body != null">body = #{body},</if>
            <if test="result != null">result = #{result},</if>
            <if test="successFlag != null">success_flag = #{successFlag},</if>
            <if test="handleFlag != null">handle_flag = #{handleFlag},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createdBy != null and createdBy != ''">created_by = #{createdBy},</if>
            <if test="createdTime != null">created_time = #{createdTime},</if>
            <if test="updatedBy != null and updatedBy != ''">updated_by = #{updatedBy},</if>
            <if test="updatedTime != null">updated_time = #{updatedTime},</if>
        </trim>
        where flow_id = #{flowId}
    </update>

</mapper>