<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paic.ehis.product.mapper.ProductManagerLogMapper">
    
    <resultMap type="ProductManagerLog" id="ProductManagerLogResult">
        <result property="serialNo"    column="serial_no"    />
        <result property="productCode"    column="product_code"    />
        <result property="bussinessStatus"    column="bussiness_status"    />
        <result property="reason"    column="reason"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectProductManagerLogVo">
        select serial_no, product_code, bussiness_status, reason, status, create_by, create_time, update_by, update_time from product_manager_log
    </sql>

    <select id="selectProductManagerLogList" parameterType="ProductManagerLog" resultMap="ProductManagerLogResult">
        <include refid="selectProductManagerLogVo"/>
        <where>  
            <if test="productCode != null  and productCode != ''"> and product_code = #{productCode}</if>
            <if test="bussinessStatus != null  and bussinessStatus != ''"> and bussiness_status = #{bussinessStatus}</if>
            <if test="reason != null  and reason != ''"> and reason = #{reason}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectProductManagerLogById" parameterType="String" resultMap="ProductManagerLogResult">
        <include refid="selectProductManagerLogVo"/>
        where serial_no = #{serialNo}
    </select>
        
    <insert id="insertProductManagerLog" parameterType="ProductManagerLog">
        insert into product_manager_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="serialNo != null">serial_no,</if>
            <if test="productCode != null and productCode != ''">product_code,</if>
            <if test="bussinessStatus != null and bussinessStatus != ''">bussiness_status,</if>
            <if test="reason != null and reason != ''">reason,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="serialNo != null">#{serialNo},</if>
            <if test="productCode != null and productCode != ''">#{productCode},</if>
            <if test="bussinessStatus != null and bussinessStatus != ''">#{bussinessStatus},</if>
            <if test="reason != null and reason != ''">#{reason},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateProductManagerLog" parameterType="ProductManagerLog">
        update product_manager_log
        <trim prefix="SET" suffixOverrides=",">
            <if test="productCode != null and productCode != ''">product_code = #{productCode},</if>
            <if test="bussinessStatus != null and bussinessStatus != ''">bussiness_status = #{bussinessStatus},</if>
            <if test="reason != null and reason != ''">reason = #{reason},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where serial_no = #{serialNo}
    </update>

    <delete id="deleteProductManagerLogById" parameterType="String">
        delete from product_manager_log where serial_no = #{serialNo}
    </delete>

    <delete id="deleteProductManagerLogByIds" parameterType="String">
        delete from product_manager_log where serial_no in 
        <foreach item="serialNo" collection="array" open="(" separator="," close=")">
            #{serialNo}
        </foreach>
    </delete>

    <update id="updateStatus" parameterType="String">
        update product_manager_log set status = 'N'
        where product_code = #{productCode} and status = 'Y'
    </update>
    
</mapper>