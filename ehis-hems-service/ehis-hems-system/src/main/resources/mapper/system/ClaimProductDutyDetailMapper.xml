<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paic.ehis.system.mapper.ClaimProductDutyDetailMapper">
    
    <resultMap type="ClaimProductDutyDetail" id="ClaimProductDutyDetailResult">
        <result property="dutyDetailCode"    column="duty_detail_code"    />
        <result property="dutyCode"    column="duty_code"    />
        <result property="planCode"    column="plan_code"    />
        <result property="riskCode"    column="risk_code"    />
        <result property="dutyDetailName"    column="duty_detail_name"    />
        <result property="applyReason"    column="apply_reason"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectClaimProductDutyDetailVo">
        select duty_detail_code, duty_code, plan_code, risk_code, duty_detail_name, apply_reason, status, create_by, create_time, update_by, update_time from claim_product_duty_detail
    </sql>

    <select id="selectClaimProductDutyDetailList" parameterType="ClaimProductDutyDetail" resultMap="ClaimProductDutyDetailResult">
        <include refid="selectClaimProductDutyDetailVo"/>
        <where>  
            <if test="planCode != null  and planCode != ''"> and plan_code = #{planCode}</if>
            <if test="riskCode != null  and riskCode != ''"> and risk_code = #{riskCode}</if>
            <if test="dutyDetailName != null  and dutyDetailName != ''"> and duty_detail_name like concat('%', #{dutyDetailName}, '%')</if>
            <if test="applyReason != null  and applyReason != ''"> and apply_reason = #{applyReason}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectClaimProductDutyDetailById" parameterType="String" resultMap="ClaimProductDutyDetailResult">
        <include refid="selectClaimProductDutyDetailVo"/>
        where duty_detail_code = #{dutyDetailCode}
    </select>
        
    <insert id="insertClaimProductDutyDetail" parameterType="ClaimProductDutyDetail">
        insert into claim_product_duty_detail
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="dutyDetailCode != null">duty_detail_code,</if>
            <if test="dutyCode != null">duty_code,</if>
            <if test="planCode != null and planCode != ''">plan_code,</if>
            <if test="riskCode != null and riskCode != ''">risk_code,</if>
            <if test="dutyDetailName != null">duty_detail_name,</if>
            <if test="applyReason != null">apply_reason,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="createBy != null and createBy != ''">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="dutyDetailCode != null">#{dutyDetailCode},</if>
            <if test="dutyCode != null">#{dutyCode},</if>
            <if test="planCode != null and planCode != ''">#{planCode},</if>
            <if test="riskCode != null and riskCode != ''">#{riskCode},</if>
            <if test="dutyDetailName != null">#{dutyDetailName},</if>
            <if test="applyReason != null">#{applyReason},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="createBy != null and createBy != ''">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateClaimProductDutyDetail" parameterType="ClaimProductDutyDetail">
        update claim_product_duty_detail
        <trim prefix="SET" suffixOverrides=",">
            <if test="dutyCode != null">duty_code = #{dutyCode},</if>
            <if test="planCode != null and planCode != ''">plan_code = #{planCode},</if>
            <if test="riskCode != null and riskCode != ''">risk_code = #{riskCode},</if>
            <if test="dutyDetailName != null">duty_detail_name = #{dutyDetailName},</if>
            <if test="applyReason != null">apply_reason = #{applyReason},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="createBy != null and createBy != ''">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where duty_detail_code = #{dutyDetailCode}
    </update>

    <delete id="deleteClaimProductDutyDetailById" parameterType="String">
        delete from claim_product_duty_detail where duty_detail_code = #{dutyDetailCode}
    </delete>

    <delete id="deleteClaimProductDutyDetailByIds" parameterType="String">
        delete from claim_product_duty_detail where duty_detail_code in 
        <foreach item="dutyDetailCode" collection="array" open="(" separator="," close=")">
            #{dutyDetailCode}
        </foreach>
    </delete>
    
</mapper>