<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paic.ehis.system.mapper.ClaimProductFeeitemMapper">
    
    <resultMap type="ClaimProductFeeitem" id="ClaimProductFeeitemResult">
        <result property="feeitemCode"    column="feeitem_code"    />
        <result property="dutyDetailCode"    column="duty_detail_code"    />
        <result property="dutyCode"    column="duty_code"    />
        <result property="planCode"    column="plan_code"    />
        <result property="riskCode"    column="risk_code"    />
        <result property="feeitemName"    column="feeitem_name"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectClaimProductFeeitemVo">
        select feeitem_code, duty_detail_code, duty_code, plan_code, risk_code, feeitem_name, status, create_by, create_time, update_by, update_time from claim_product_feeitem
    </sql>

    <select id="selectClaimProductFeeitemList" parameterType="ClaimProductFeeitem" resultMap="ClaimProductFeeitemResult">
        <include refid="selectClaimProductFeeitemVo"/>
        <where>  
            <if test="dutyCode != null  and dutyCode != ''"> and duty_code = #{dutyCode}</if>
            <if test="planCode != null  and planCode != ''"> and plan_code = #{planCode}</if>
            <if test="riskCode != null  and riskCode != ''"> and risk_code = #{riskCode}</if>
            <if test="feeitemName != null  and feeitemName != ''"> and feeitem_name like concat('%', #{feeitemName}, '%')</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectClaimProductFeeitemById" parameterType="String" resultMap="ClaimProductFeeitemResult">
        <include refid="selectClaimProductFeeitemVo"/>
        where feeitem_code = #{feeitemCode}
    </select>
        
    <insert id="insertClaimProductFeeitem" parameterType="ClaimProductFeeitem">
        insert into claim_product_feeitem
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="feeitemCode != null">feeitem_code,</if>
            <if test="dutyDetailCode != null">duty_detail_code,</if>
            <if test="dutyCode != null and dutyCode != ''">duty_code,</if>
            <if test="planCode != null and planCode != ''">plan_code,</if>
            <if test="riskCode != null and riskCode != ''">risk_code,</if>
            <if test="feeitemName != null">feeitem_name,</if>
            <if test="status != null and status != ''">status,</if>
            <if test="createBy != null and createBy != ''">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="feeitemCode != null">#{feeitemCode},</if>
            <if test="dutyDetailCode != null">#{dutyDetailCode},</if>
            <if test="dutyCode != null and dutyCode != ''">#{dutyCode},</if>
            <if test="planCode != null and planCode != ''">#{planCode},</if>
            <if test="riskCode != null and riskCode != ''">#{riskCode},</if>
            <if test="feeitemName != null">#{feeitemName},</if>
            <if test="status != null and status != ''">#{status},</if>
            <if test="createBy != null and createBy != ''">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateClaimProductFeeitem" parameterType="ClaimProductFeeitem">
        update claim_product_feeitem
        <trim prefix="SET" suffixOverrides=",">
            <if test="dutyDetailCode != null">duty_detail_code = #{dutyDetailCode},</if>
            <if test="dutyCode != null and dutyCode != ''">duty_code = #{dutyCode},</if>
            <if test="planCode != null and planCode != ''">plan_code = #{planCode},</if>
            <if test="riskCode != null and riskCode != ''">risk_code = #{riskCode},</if>
            <if test="feeitemName != null">feeitem_name = #{feeitemName},</if>
            <if test="status != null and status != ''">status = #{status},</if>
            <if test="createBy != null and createBy != ''">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where feeitem_code = #{feeitemCode}
    </update>

    <delete id="deleteClaimProductFeeitemById" parameterType="String">
        delete from claim_product_feeitem where feeitem_code = #{feeitemCode}
    </delete>

    <delete id="deleteClaimProductFeeitemByIds" parameterType="String">
        delete from claim_product_feeitem where feeitem_code in 
        <foreach item="feeitemCode" collection="array" open="(" separator="," close=")">
            #{feeitemCode}
        </foreach>
    </delete>
    
</mapper>