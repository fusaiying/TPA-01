<mapper namespace="com.paic.ehis.system.mapper.HmpBenefitCustomerMapper">
    
    <resultMap type="HmpBenefitCustomer" id="HmpBenefitCustomerResult">
        <result property="customerid"    column="customerid"    />
        <result property="name"    column="name"    />
        <result property="sex"    column="sex"    />
        <result property="birthday"    column="birthday"    />
        <result property="idtype"    column="idtype"    />
        <result property="idcardno"    column="idcardno"    />
        <result property="occupation"    column="occupation"    />
        <result property="socialsecurityno"    column="socialsecurityno"    />
        <result property="nationality"    column="nationality"    />
        <result property="effectivedate"    column="effectivedate"    />
