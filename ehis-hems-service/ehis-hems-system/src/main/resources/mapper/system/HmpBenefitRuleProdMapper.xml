<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paic.ehis.system.mapper.HmpBenefitRuleProdMapper">
    
    <resultMap type="HmpBenefitRuleProd" id="HmpBenefitRuleProdResult">
        <result property="rulecode"    column="rulecode"    />
        <result property="productcode"    column="productcode"    />
        <result property="productname"    column="productname"    />
        <result property="servicecode"    column="servicecode"    />
        <result property="servicename"    column="servicename"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectHmpBenefitRuleProdVo">
        select rulecode, productcode, productname, servicecode, servicename, create_by, create_time, update_by, update_time from hmp_benefit_rule_prod
    </sql>

    <select id="selectHmpBenefitRuleProdList" parameterType="HmpBenefitRuleProd" resultMap="HmpBenefitRuleProdResult">
        <include refid="selectHmpBenefitRuleProdVo"/>
        <where>  
            <if test="productname != null  and productname != ''"> and productname like concat('%', #{productname}, '%')</if>
            <if test="servicename != null  and servicename != ''"> and servicename like concat('%', #{servicename}, '%')</if>
        </where>
    </select>
    
    <select id="selectHmpBenefitRuleProdById" parameterType="String" resultMap="HmpBenefitRuleProdResult">
        <include refid="selectHmpBenefitRuleProdVo"/>
        where rulecode = #{rulecode}
    </select>
        
    <insert id="insertHmpBenefitRuleProd" parameterType="HmpBenefitRuleProd">
        insert into hmp_benefit_rule_prod
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="rulecode != null">rulecode,</if>
            <if test="productcode != null">productcode,</if>
            <if test="productname != null">productname,</if>
            <if test="servicecode != null">servicecode,</if>
            <if test="servicename != null">servicename,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="rulecode != null">#{rulecode},</if>
            <if test="productcode != null">#{productcode},</if>
            <if test="productname != null">#{productname},</if>
            <if test="servicecode != null">#{servicecode},</if>
            <if test="servicename != null">#{servicename},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateHmpBenefitRuleProd" parameterType="HmpBenefitRuleProd">
        update hmp_benefit_rule_prod
        <trim prefix="SET" suffixOverrides=",">
            <if test="productcode != null">productcode = #{productcode},</if>
            <if test="productname != null">productname = #{productname},</if>
            <if test="servicecode != null">servicecode = #{servicecode},</if>
            <if test="servicename != null">servicename = #{servicename},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where rulecode = #{rulecode}
    </update>

    <delete id="deleteHmpBenefitRuleProdById" parameterType="String">
        delete from hmp_benefit_rule_prod where rulecode = #{rulecode}
    </delete>

    <delete id="deleteHmpBenefitRuleProdByIds" parameterType="String">
        delete from hmp_benefit_rule_prod where rulecode in 
        <foreach item="rulecode" collection="array" open="(" separator="," close=")">
            #{rulecode}
        </foreach>
    </delete>
    
</mapper>